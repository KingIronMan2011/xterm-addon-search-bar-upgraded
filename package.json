{
  "name": "xterm-addon-search-bar-upgraded",
  "version": "0.2.5",
  "global": "SearchBarAddon",
  "main": "lib/xterm-addon-search-bar.js",
  "module": "lib/xterm-addon-search-bar.esm.js",
  "unpkg": "lib/xterm-addon-search-bar.min.js",
  "types": "typings/index.d.ts",
  "keywords": [
    "xterm",
    "xterm-addon",
    "search-bar",
    "xterm-search-addon",
    "xterm-search-bar-addon"
  ],
  "author": "KingIronMan2011",
  "url": "https://github.com/KingIronMan2011/xterm-addon-search-bar-upgraded",
  "license": "MIT",
  "scripts": {
    "cz": "git-cz",
    "clean": "rimraf lib",
    "start": "yarn run clean && cross-env NODE_ENV=development rollup -w -c scripts/rollup.config.dev.js",
    "build": "yarn run clean && cross-env NODE_ENV=production rollup -c scripts/rollup.config.prod.js",
    "format": "prettier --write .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KingIronMan2011/xterm-addon-search-bar-upgraded.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "src",
    "lib",
    "typings"
  ],
  "peerDependencies": {
    "@xterm/addon-search": "^0.13.0",
    "@xterm/xterm": "^5.3.0"
  },
  "dependencies": {
    "core-js": "^3.34.0",
    "postcss": "^8.5.6",
    "rxjs-compat": "^6.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.10.2",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/xterm": "^5.5.0",
    "ansi_up": "^6.0.6",
    "commitizen": "^4.3.1",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^10.1.0",
    "cz-customizable": "^7.5.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "monaco-editor": "^0.55.1",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "serve": "^14.2.5",
    "typescript": "^5.9.3"
  }
